<div class="panel large-container">
  <!-- Default panel contents -->
  <div class="panel-heading large-heading">Categories</div>

  <!-- Table -->
  <table class="table">
    <tr>
      <th>Title</th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>
    <% _.each(categories, function(category) { %>
        <tr>
          <td><%= category.title %></td>
          <td><a href="/admin/editCategory?id=<%= category.id %>">Edit</a></td>     
          <td><a href="/admin/deleteCategory?id=<%= category.id %>">Edit</a></td>
        </tr>
    <% }) %>
  </table>
  <a href="/admin/createCategory">
    <h5>Add Category</h5>
  </a>
</div>
