<div class="boxes-container">
  <% _.each(categories, function(category) { %>

    <a href="/admin/byCategory?id=<%= category.id %>">
      <div class="section-box">
        <h2><%= category.title %></h2>
        <p><%= category.brief %></p>
      </div>
    </a>
  <% }) %>
  <a href="/category/createCategory">
    <div class="section-box">
      <h2>(+) Tutorials Category</h2>
    </div>
  </a>
</div>
